<view class='container'>
  <view class='arrow title'>
    <view class='ml3-5'><image src='../../images/address.png' class=''></image>杭州市</view>
  </view>
  <swiper style='width:100%' autoplay='true' class='mt1-5'>
    <block wx:for="{{img}}" wx:key="index">
      <swiper-item><image src='{{item.url}}' mode="widthFix"></image></swiper-item>
    </block>
  </swiper>
  <view class='sub-main' wx:for="{{img}}" wx:key="index">
    <image src='{{item.url}}' mode='widthFix'></image>
  </view>
  <view class='refer-p'>
    <navigator url='../detail/index'>
      <image src='../../images/customer.png' class='refer-size' mode='widthFix'></image>
    </navigator>
  </view>
</view>
